<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="top"
    android:orientation="horizontal"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" >

    <Button
        android:id="@+id/bttnPrA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="1dp"
        android:height="122dp"
        android:maxHeight="122dp"
        android:maxWidth="167dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:width="167dp" />

    <Button
        android:id="@+id/bttnPrO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="1dp"
        android:layout_marginLeft="3dp"
        android:height="122dp"
        android:maxHeight="122dp"
        android:maxWidth="167dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:width="167dp" />

    <Button
        android:id="@+id/bttnPrT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/bttnPrF"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="1dp"
        android:layout_marginRight="3dp"
        android:height="122dp"
        android:maxHeight="122dp"
        android:maxWidth="167dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:width="167dp" />

    <Button
        android:id="@+id/bttnPrSpace"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:height="54dp"
        android:maxHeight="54dp"
        android:maxWidth="308dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:text="BARRA ESPACIADORA"
        android:width="308dp" />

    <Button
        android:id="@+id/bttnPrPoint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/bttnPrA"
        android:layout_alignParentLeft="false"
        android:layout_alignTop="@dimen/activity_horizontal_margin"
        android:layout_centerVertical="true"
        android:layout_marginTop="208dp"
        android:height="64dp"
        android:maxHeight="64dp"
        android:maxWidth="91dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:text="."
        android:width="91dp" />

    <Button
        android:id="@+id/bttnPrDelete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/bttnPrPoint"
        android:layout_alignRight="@+id/bttnPrF"
        android:height="64dp"
        android:maxHeight="64dp"
        android:maxWidth="91dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:text="Borrar"
        android:width="91dp" />

    <Button
        android:id="@+id/bttnPrK"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/bttnPrDelete"
        android:layout_alignBottom="@+id/bttnPrDelete"
        android:layout_centerHorizontal="true"
        android:height="122dp"
        android:maxHeight="122dp"
        android:maxWidth="167dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:width="167dp" />

    <Button
        android:id="@+id/bttnPrNO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/bttnPrSI"
        android:layout_alignBottom="@+id/bttnPrSI"
        android:layout_alignRight="@+id/bttnPrSpace"
        android:height="62dp"
        android:maxHeight="62dp"
        android:maxWidth="91dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:text="NO"
        android:width="91dp" />

    <Button
        android:id="@+id/bttnPrF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/bttnPrA"
        android:layout_marginRight="3dp"
        android:height="122dp"
        android:maxHeight="122dp"
        android:maxWidth="167dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:width="167dp" />

    <Button
        android:id="@+id/bttnPrSI"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/bttnPrSpace"
        android:layout_alignTop="@+id/bttnPrO"
        android:height="62dp"
        android:maxHeight="62dp"
        android:maxWidth="91dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:text="SI"
        android:width="91dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/bttnPrO"
        android:layout_alignLeft="@+id/bttnPrSI"
        android:layout_alignRight="@+id/bttnPrNO"
        android:ems="10"
        android:paddingLeft="50dp"
        android:paddingTop="20dp" >

        <requestFocus />
    </EditText>

    

</RelativeLayout>
