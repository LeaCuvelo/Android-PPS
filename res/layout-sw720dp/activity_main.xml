<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="top"
    android:orientation="horizontal"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" >

    <Button
        android:id="@+id/bttnPrO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="2dp"
        android:layout_marginLeft="4dp"
        android:height="203dp"
        android:maxHeight="203dp"
        android:maxWidth="267dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:width="267dp"
        style="@style/boton_style"
         />

    <Button
        android:id="@+id/bttnPrT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/bttnPrF"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="2dp"
        android:layout_marginRight="4dp"
        android:height="203dp"
        android:maxHeight="203dp"
        android:maxWidth="267dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:width="267dp"
        style="@style/boton_style"
         />

    <Button
        android:id="@+id/bttnPrSpace"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:height="91dp"
        android:maxHeight="91dp"
        android:maxWidth="493dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:text="BARRA ESPACIADORA"
        android:width="493dp" />

    <Button
        android:id="@+id/bttnPrPoint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/bttnPrA"
        android:layout_alignParentLeft="false"
        android:layout_alignTop="@dimen/activity_horizontal_margin"
        android:layout_centerVertical="true"
        android:layout_marginTop="346dp"
        android:height="107dp"
        android:maxHeight="107dp"
        android:maxWidth="145dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:text="."
        android:width="145dp" />

    <Button
        android:id="@+id/bttnPrDelete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/bttnPrPoint"
        android:layout_alignRight="@+id/bttnPrF"
        android:height="107dp"
        android:maxHeight="107dp"
        android:maxWidth="145dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:text="Borrar"
        android:width="145dp" />

    <Button
        android:id="@+id/bttnPrK"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/bttnPrDelete"
        android:layout_alignBottom="@+id/bttnPrDelete"
        android:layout_centerHorizontal="true"
        android:height="203dp"
        android:maxHeight="203dp"
        android:maxWidth="267dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:width="267dp"
        style="@style/boton_style"
         />

    <Button
        android:id="@+id/bttnPrNO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/bttnPrSI"
        android:layout_alignBottom="@+id/bttnPrSI"
        android:layout_alignRight="@+id/bttnPrSpace"
        android:height="103dp"
        android:maxHeight="103dp"
        android:maxWidth="145dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:text="NO"
        android:width="145dp" />

    <Button
        android:id="@+id/bttnPrF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/bttnPrA"
        android:layout_marginRight="4dp"
        android:height="203dp"
        android:maxHeight="203dp"
        android:maxWidth="267dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:width="267dp"
        style="@style/boton_style"
         />

    <Button
        android:id="@+id/bttnPrSI"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/bttnPrSpace"
        android:layout_alignTop="@+id/bttnPrO"
        android:height="103dp"
        android:maxHeight="103dp"
        android:maxWidth="145dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:text="SI"
        android:width="145dp" />

    <Button
        android:id="@+id/bttnPrA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/bttnPrSpace"
        android:layout_marginLeft="4dp"
        android:height="203dp"
        android:maxHeight="203dp"
        android:maxWidth="267dp"
        android:minHeight="1dp"
        android:minWidth="1dp"
        android:width="267dp"
        style="@style/boton_style"
         />
    
    <EditText
        android:id="@+id/editText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/bttnPrO"
        android:layout_alignLeft="@+id/bttnPrSI"
        android:layout_alignRight="@+id/bttnPrNO"
        android:ems="10"
        android:maxLines="1"
        android:scrollbars="vertical" >

        <requestFocus />
    </EditText>


    
</RelativeLayout>
